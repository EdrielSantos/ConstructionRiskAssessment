<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homefragment">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="164dp"
        android:layout_height="63dp"
        android:fontFamily="@font/roboto_black"
        android:text="Karl Vrad Nuyda"
        android:textColor="#304FFE"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="288dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="210dp"
        android:layout_height="124dp"
        android:fontFamily="@font/roboto_black"
        android:shadowColor="#1B1919"
        android:text="Christian Rabadon                 Intelligent Systems"
        android:textColor="#25916F"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="457dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="154dp"
        android:layout_height="145dp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="44dp"
        android:src="@drawable/santos"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="154dp"
        android:layout_height="145dp"
        android:layout_below="@id/imageView"
        android:src="@drawable/nuyda"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="244dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="154dp"
        android:layout_height="145dp"
        android:layout_below="@id/imageView2"
        android:src="@drawable/rabadon"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="436dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="164dp"
        android:layout_height="63dp"
        android:fontFamily="@font/roboto_black"
        android:text="Edriel Santos                 Intelligent Systems"
        android:textColor="#F44336"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="129dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
